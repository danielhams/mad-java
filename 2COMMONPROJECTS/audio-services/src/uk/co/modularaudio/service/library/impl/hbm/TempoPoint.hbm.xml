<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping package="uk.co.modularaudio.service.library">
		<class name="TempoPoint" table="TEMPO_POINT">
			<id name="tempoPointId" column="TEMPO_POINT_ID">
				<generator class="sequence">
					<param name="sequence">TEMPO_POINT_SEQ</param>
				</generator>
			</id>
			<property name="framePosition" column="FRAME_POSITION" type="long"/>
		</class>
	</hibernate-mapping>
